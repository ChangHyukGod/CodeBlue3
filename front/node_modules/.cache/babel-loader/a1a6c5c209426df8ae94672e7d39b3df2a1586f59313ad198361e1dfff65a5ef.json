rrorMessage: \"\", // 에러 메시지 상태 관리\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        let response = await MemberService.login(this.user);\r\n        console.log(response.data);\r\n\r\n        this.$store.state.loggedIn = true;\r\n\r\n        localStorage.setItem(\"user\", JSON.stringify(response.data));\r\n\r\n        this.errorMessage = \"\"; // 에러 메시지 초기화\r\n\r\n        this.$router.push(\"/\");\r\n      } catch (error) {\r\n        this.$store.state.loggedIn = false;\r\n\r\n        this.errorMessage = `\r\n          로그인에 실패했습니다.<br />\r\n          아이디 또는 비밀번호를 맞게 입력해 주세요.\r\n        `;\r\n\r\n        console.log(error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.$store.state.loggedIn === true) {\r\n      this.$router.push(\"/\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.alert {\r\n  font-size: 0.9rem;\r\n  border-radius: 5px;\r\n  padding: 10px 15px;\r\n}\r\n.alert-danger {\r\n  background-color: #f8d7da;\r\n  color: #842029;\r\n  border: 1px solid #f5c2c7;\r\n}\r\n.alert .bi {\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.btn-custom {\r\n  background-color: black; /* 배경색: 검은색 */\r\n  color: white;            /* 글자색: 흰색 */\r\n  border-radius: 50px;     /* 타원형 효과 */\r\n  padding: 10px 20px;      /* 버튼 내부 여백 */\r\n  font-size: 16px;         /* 글씨 크기 */\r\n  border: none;            /* 테두리 제거 */\r\n  transition: background-color 0.3s ease; /* 배경 전환 효과 */\r\n}\r\n\r\n/* Hover 효과 */\r\n.btn-custom:hover {\r\n  background-color: #333; /* 더 밝은 검은색 */\r\n}\r\n</style>\r\n"],"mappings":";AAiEA,OAAOA,aAAY,MAAO,+BAA+B;AAEzD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MAEDC,YAAY,EAAE,EAAE,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,IAAIC,QAAO,GAAI,MAAMR,aAAa,CAACO,KAAK,CAAC,IAAI,CAACL,IAAI,CAAC;QACnDO,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACP,IAAI,CAAC;QAE1B,IAAI,CAACU,MAAM,CAACC,KAAK,CAACC,QAAO,GAAI,IAAI;QAEjCC,YAAY,CAACC,OAAO,CAAC,